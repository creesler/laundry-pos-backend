{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "installCommand": "npm install --force --no-optional --platform=linux --arch=x64"
}